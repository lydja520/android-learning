<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ydc.androidlearning">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.ActivityLearn" />
        <activity android:name=".activitys.IntentLearn" />
        <activity android:name=".activitys.IntentLearnDistint" />
        <activity android:name=".activitys.ActivityLearnJumpActivity" />
        <activity android:name=".activitys.ComponentLearnActivity" />
    </application>

</manifest>